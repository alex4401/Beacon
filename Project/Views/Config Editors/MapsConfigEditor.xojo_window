#tag WindowCode
	#tag Event
		Sub RestoreToDefault()
		  Self.Document.MapCompatibility = Beacon.Maps.TheIsland.Mask
		End Sub
	#tag EndEvent

	#tag Event
		Sub SetupUI()
		  Dim Maps() As Beacon.Map = Beacon.Maps.All
		  For Each Map As Beacon.Map In Maps
		    Dim Ctl As CheckBox = Self.MapCheckboxes(Map.Mask)
		    If Ctl = Nil Then
		      Continue
		    End If
		    
		    Ctl.Value = Self.Document.SupportsMap(Map)
		  Next
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Function ConfigLabel() As String
		  Return "Maps"
		End Function
	#tag EndMethod


#tag EndWindowCode

#tag Events MapCheckboxes
	#tag Event
		Sub ValueChanged(index as Integer)
		  If Self.SettingUp Then
		    Return
		  End If
		  
		  Self.SettingUp = True
		  Dim MaskValue As Integer = Index
		  If Me.Value Then
		    Self.Document.MapCompatibility = Self.Document.MapCompatibility Or MaskValue
		  Else
		    Self.Document.MapCompatibility = Self.Document.MapCompatibility And Not MaskValue
		  End If
		  Self.Changed = True
		  Self.SettingUp = False
		End Sub
	#tag EndEvent
#tag EndEvents
